---
layout: default
---
<img class="ob_Image--large ob_Image" src="{{ site.imgPath }}{{ heroImage }}" alt="">
<div class="cp_Text--title cp_Text">
  <h1 class="cp_Text__pageTitle hd_Han">{{ title }}</h1>
</div>
<div class="cp_Text--case cp_Text">
  <div class="cp_Text__inner tx_Prose--case tx_Prose">
    {{ content | safe }} 
  </div>
</div>

{%- for image in gallery -%}
  <img class="ob_Image--{{ image.size }} ob_Image" src="{{ site.imgPath }}{{ image.path }}" alt="">
{%- endfor -%}