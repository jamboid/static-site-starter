{
  "name": "static-site-starter",
  "version": "1.0.0",
  "description": "Boilerplate site structure for creating static websites using Eleventy and Webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --kill-others \"npx eleventy --watch\" \"webpack --mode development --watch\"",
    "dashdev": "concurrently --kill-others \"npx eleventy --watch\" \"webpack-dashboard -- webpack --mode development --watch\"",
    "build": "concurrently --kill-others \"npx eleventy --watch\" \"webpack --mode production --watch\"",
    "dashbuild": "webpack-dashboard -- webpack --mode production --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "debounce": "^1.2.0",
    "delegate": "^3.2.0",
    "imagesloaded": "^4.1.4",
    "pubsub-js": "^1.7.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "cssnano": "^4.1.7",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^2.0.0",
    "node-sass": "^4.10.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-notifier": "^1.7.0"
  }
}
